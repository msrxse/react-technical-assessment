{
  "name": "default-scaffold",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --watch --coverage",
    "test:ui": "vitest --watch --coverage --ui",
    "preview": "vite preview",
    "test": "vitest",
    "lint": "eslint src --no-inline-config --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "npm run lint -- --fix",
    "lint:css": "stylelint **/*.css --aei --color",
    "prettier": "prettier src --check",
    "prettier:fix": "npm run prettier -- --write",
    "format": "npm run prettier && npm run lint",
    "format:fix": "npm run prettier:fix && npm run lint:fix",
    "build": "tsc -p tsconfig.prod.json && vite build"
  },
  "dependencies": {
    "@emotion/react": "^11.11.3",
    "@emotion/styled": "^11.11.0",
    "@monaco-editor/react": "^4.6.0",
    "@mui/material": "^5.15.7",
    "@mui/x-tree-view": "^6.17.0",
    "@tanstack/react-query": "^5.17.1",
    "axios": "^1.6.5",
    "monaco-editor": "^0.45.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "seti-icons": "^0.0.4"
  },
  "devDependencies": {
    "@tanstack/eslint-plugin-query": "^5.17.1",
    "@testing-library/jest-dom": "^6.2.0",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.2",
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@types/jest": "^29.5.11",
    "@types/react": "^18.2.46",
    "@types/react-dom": "^18.2.18",
    "@typescript-eslint/eslint-plugin": "^6.17.0",
    "@typescript-eslint/parser": "^6.17.0",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "@vitest/coverage-v8": "^1.1.3",
    "@vitest/ui": "^1.1.3",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "happy-dom": "^12.10.3",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "msw": "^2.0.11",
    "prettier": "^3.1.1",
    "stylelint": "^16.1.0",
    "stylelint-config-prettier": "^9.0.5",
    "typescript": "^5.2.2",
    "vite": "^5.0.8",
    "vite-plugin-monaco-editor": "^1.1.0",
    "vite-tsconfig-paths": "^4.2.3",
    "vitest": "^1.0.1"
  },
  "msw": {
    "workerDirectory": "public"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
